generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Event {
  eventId   Int    @id @default(autoincrement()) @map("event_id")
  eventName String @map("event_name")

  @@map("event")
}
