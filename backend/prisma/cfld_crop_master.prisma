generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model FLDCropMaster {
  cfldId    Int    @id @default(autoincrement()) @map("cfld_id")
  Season    String @map("season")
  Type      String @map("type")
  CropName  String @map("crop_name")

  @@map("fld_crop_master")
}
